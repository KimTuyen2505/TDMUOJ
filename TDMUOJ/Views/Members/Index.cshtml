@model dynamic
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link rel="stylesheet" href="~/Content/css/Members.css" />
        <div class="content-left">
            <h1>Bảng xếp hạng</h1>
            <br />
            <hr />
            <br />
            <div class="table">
                <div class="head-table">
                    <div class="rank">Hạng</div>
                    <div class="name"><p>Tên truy cập</p></div>
                    <div class="problem"><p>Số bài</p></div>
                </div>
                @foreach (var member in Model)
                {
                    <div class="child-table">
                        <div class="rank">@member.rank</div>
                        <div class="name">
                            <a href="@Url.Action("Index", "Profile", new {id = member.id})">@member.name</a>
                        </div>
                        <div class="problem">@member.numberOfAccepted</div>
                    </div>
                }
            </div>
        </div>
